<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.writenumber">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".itempages.SecondActivity" />
        <activity android:name=".itempages.ThirdActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".itempages.FouthActivity"></activity>
        <activity android:name=".itempages.OneActivity"></activity>
    </application>

</manifest>